<h1>Hi <%= name %></h1>
<table>
  <thead>
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Email</th>
      <th>Role</th>
      <th>Action</th>
    </tr>
  </thead>
  <tbody>
    <% users.forEach(user => { %>
    <tr>
      <td><%= user.firstname %></td>
      <td><%= user.lastname %></td>
      <td><%= user.email %></td>
      <td><%= user.role %></td>
      <td>
        <a href="/admin/editUser/<%= user.id %>">Edit</a>
        <a href="/admin/deleteUser/<%= user.id %>">Delete</a>
      </td>
    </tr>

    <% }); %>
  </tbody>
</table>
<a href="/admin/addUser">Add new user?</a>
<br />
<a href="/admin/logout">Logout</a>